package tools;

import java.util.HashMap;

import QQview.Qqfriend;

public class friend_manage {

	HashMap fh=new HashMap<String,Qqfriend>();
	public void add(String s,Qqfriend qf)
	{
		fh.put(s, qf);
	}
	public Qqfriend getQqfriend(String s) {
		return  (Qqfriend)fh.get(s);
		
	}
}
